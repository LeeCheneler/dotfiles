{
  "includes": ["**/*.md", "**/*.json", "**/*.yaml", "**/*.yml"],
  "excludes": ["**/node_modules"],
  "plugins": [
    "https://plugins.dprint.dev/markdown-0.17.8.wasm",
    "https://plugins.dprint.dev/json-0.19.4.wasm"
  ],
  "markdown": {
    "lineWidth": 120
  },
  "json": {
    "indentWidth": 2
  }
}
